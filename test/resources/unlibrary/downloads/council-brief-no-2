var textPosition = "k-ltr";
var numberPosition = "";

// returns the value of the lang attribute - e.g. "en", "fr"
var htmlLang = $("html").attr("lang");

// defines the index of the language - 0 is EN and fallback
var cultureIndex = 0;

if(htmlLang == "ar")
{
    cultureIndex = 1;
    textPosition = "k-rtl";
}
if(htmlLang == "es")
{
    cultureIndex = 2;
}
if(htmlLang == "fr")
{
    cultureIndex = 3;
}
if(htmlLang == "ru")
{
    cultureIndex = 4;
}
if(htmlLang == "zh")
{
    cultureIndex = 5;
}

// link to the country overview page label
var breadcrumb_overview_label = [
    "Overview", 
    "نظرة عامة",
    "Visión de conjunto",
    "Vue d'ensemble",
    "Обзор",
    "概觀"
];

$(document).ready(function () {
    var publicationHubLinkLabel = breadcrumb_overview_label[cultureIndex];

    $('.sf-breadscrumb.breadcrumb li a').each(function () {
        if ($(this).text() == "i" || $(this).text() == "m") {
            $(this).text(publicationHubLinkLabel);
        }
        if ($(this).text().toLowerCase() == "item") {
            $(this).next().remove();
            $(this).remove();
        }
    });
  
  $('.sf-breadscrumb.breadcrumb li.active').each(function () {        
        if ($(this).text().toLowerCase() == "item") {
            $(this).next().remove();
            $(this).remove();
        }

	
    });
});
</script><script type="text/javascript">
$(document).ready(function() {
	moveLanguageSelector();
});

function moveLanguageSelector() {

       var languageSelectorElement = document.querySelector(".language-selector.language-selector-page");

        var hasAvailableLanguages = $(".language-selector.language-selector-page ul.sf-lang-selector.list-inline li a[style='display: inline-block;']");

        var sidebarWrapperDiv = document.querySelector('.dynamic-content__details');

        var languageSelectorDetailDiv = document.createElement("div");
        languageSelectorDetailDiv.className = "detail translations-placeholder";

        if (hasAvailableLanguages.length) {
            var languageSelectorLabelDiv = document.createElement("div");
            languageSelectorLabelDiv.className = "label";
            languageSelectorLabelDiv.innerHTML = "Language(s) available";

            languageSelectorDetailDiv.append(languageSelectorLabelDiv);
        }      

        sidebarWrapperDiv.append(languageSelectorDetailDiv);

        sidebarWrapperDiv.append(languageSelectorElement);
    }
</script><script type="text/javascript">
$(document).ready(function() {

  var languageSelectorWrapper = $(".language-selector.language-selector-page");
  languageSelectorWrapper.attr("id","language-selector-item-details");

  languageSelectorWrapper.html("");
  var htmlLang = $("html").attr("lang");
  
  $("body").children("input[data-sf-role]").each(function (key, value) {
    var itemUrl = $(this).attr('value');
    var itemCulture = $(this).attr("data-sf-role");
    var isDetailRootPage = itemUrl.match(/detail$/);
    var isDetailsRootPage = itemUrl.match(/details$/);
    var isItemRootPage = itemUrl.match(/item$/);
    var ishomePage = itemUrl.match(/home$/);
    
    var isCurrentCulture = $(this).attr("data-sf-role") == htmlLang;
    var languageCulture = $(".sf-lang-selector.language-selector select option[value='" + itemCulture + "']").attr("value");

    if (key <= 6) {
      
    if(languageCulture != undefined && isDetailRootPage == null && isDetailsRootPage == null && ishomePage == null && isItemRootPage == null && !isCurrentCulture) {

      
      var languageDisplayName = $(".sf-lang-selector.language-selector select option[value='" + itemCulture + "']").text();
      
      var languageButtonWrapper = $("<div/>", {
          "class": "button button-blue-border"
      });
      
      languageButtonWrapper.appendTo(languageSelectorWrapper);
      
      $("<a/>", {
        "class": "link",
        text: languageDisplayName,
        "href": itemUrl,
        "data-language-variation": itemCulture
      }).appendTo(languageButtonWrapper);
     }
    }
  });
});


