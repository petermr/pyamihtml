            var currentScript = d.currentScript || d.head.querySelector('script.js-entry');

            
            function catchNoModuleSupport() {
                var scriptEl = d.createElement('script');
                return (!('noModule' in scriptEl) && 'onbeforeload' in scriptEl)
            }

            var headScripts = [
                {'src': '/oscar-static/js/polyfill-es5-bundle-a5eb43b007.js', 'async': false},
                {'src': '/oscar-static/js/airbrake-es5-bundle-92469acbab.js', 'async': false},
            ];

            var bodyScripts = [
                
                    {'src': '/oscar-static/js/app-es5-bundle-4d2b75f9b1.js', 'async': false, 'module': false},
                    {'src': '/oscar-static/js/app-es6-bundle-222a2cce1a.js', 'async': false, 'module': true}
                
                
                
